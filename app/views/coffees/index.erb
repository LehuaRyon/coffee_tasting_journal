<h1>Coffees</h1>

    <% @coffees.each do |c| %>
        <p><%= c.name %> - <%= c.roaster %> - <%= c.producer %> - <%= c.variety %> - <%= c.process %> - <%= c.notes %>
            <a href="/coffees/<%= c.id %>">Edit</a>
        </p>
    <% end %>

<p><a href="/coffees/new">Create New Coffee Log</a></p>
